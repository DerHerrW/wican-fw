{
  "title": "WiCan Vehicle Profile",
  "description": "Data about various models of cars and their settings",
  "type": "object",
  "properties": {
    "car_model": {
      "description": "Name of car model, should include Make and Model(s)",
      "type": "string"
    },
    "init": {
      "description": "Init String, don't forget the final ;",
      "type": "string"
    },
    "pids": {
      "description": "Array of PID's and their parameters",
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": [
          "pid"
        ],
        "properties": {
          "pid": {
            "description": "The PID for this group of parameters",
            "type": "string"
          },
          "parameters": {
            "description": "Object of Paramater and Expressions",
            "type": "object",
            "propertyNames": {
              "enum": [
                "SOC",
                "SOC_D",
                "HV_V",
                "LV_V",
                "BATT_TEMP",
                "BATT_CAPACITY",
                "SOH",
                "TYRE_P_FL",
                "TYRE_P_FR",
                "TYRE_P_RL",
                "TYRE_P_RR",
                "ODOMETER",
                "CHARGER_CONNECTED",
                "CHARGING",
                "CHARGING_DC",
                "HV_CAPACITY",
                "HV_CAPACITY_KWH",
                "HV_CAPACITY_R",
                "HV_AV",
                "AC_P",
                "RANGE",
                "OILCH_DIS",
                "ENGINE_RPM",
                "SPEED",
                "COOLANT_TMP",
                "FUEL",
                "FUEL_PRESSURE",
                "INTAKE_AIR_TMP",
                "THROTTLE",
                "MAF",
                "STFT",
                "TMP_A",
                "AC_C_C",
                "AC_C_V"
              ]
            }
          }
        }
      }
    }
  }
}